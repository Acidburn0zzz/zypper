#: ../src/zypper-keyring-callbacks.h:163 ../src/zypper-misc.cc:1402
msgid "Continue?"
msgstr "Продолжить?"

#: ../src/zypper-keyring-callbacks.h:143
#, c-format
msgid "Unknown digest %s for file %s."
msgstr "Неизвестный дайджест %s для файла %s."

