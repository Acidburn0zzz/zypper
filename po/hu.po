msgstr "Új telepítési forrás- vagy csomagaláíró kulcs érkezett:"

#: ../src/callbacks/keyring.h:131
#, c-format
msgid "Key ID: %s"
msgstr "Kulcs azonosító: %s"

#: ../src/callbacks/keyring.h:132
#, c-format
msgid "Key Name: %s"
msgstr "Kulcs neve: %s"

