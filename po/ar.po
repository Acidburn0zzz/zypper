#: ../src/Zypper.cc:153
msgid "  Global Options:\n\t--help, -h\t\tHelp.\n\t--version, -V\t\tOutput the version number.\n\t--quiet, -q\t\tSuppress normal output, print only error\n\t\t\t\tmessages.\n\t--verbose, -v\t\tIncrease verbosity.\n\t--no-abbrev, -A\t\tDo not abbreviate text in tables.\n\t--table-style, -s\tTable style (integer).\n\t--rug-compatible, -r\tTurn on rug compatibility.\n\t--non-interactive, -n\tDo not ask anything, use default answers\n\t\t\t\tautomatically.\n\t--xmlout, -x\t\tSwitch to XML output.\n\t--reposd-dir, -D <dir>\tUse alternative repository definition files\n\t\t\t\tdirectory.\n\t--cache-dir, -C <dir>\tUse alternative meta-data cache directory.\n\t--raw-cache-dir <dir>\tUse alternative raw meta-data cache directory.\n"
msgstr "  الخيارات العامة:\n\t--help, -h\t\tHelp.\n\t--version, -V\t\tإخراج رقم الإصدار.\n\t--quiet, -q\t\tمنع الإخراج العادي، طباعة رسائل\n\t\t\t\tالخطأ فقط.\n\t--verbose, -v\t\tزيادة الإسهاب.\n\t--no-abbrev, -A\t\tعدم اختصار النص في جداول.\n\t--table-style, -s\tنمط الجدول (عدد صحيح).\n\t--rug-compatible, -r\tتشغيل توافق rug.\n\t--non-interactive, -n\tعدم السؤال عن أي شيء، استخدام الإجابات\n\t\t\t\tالافتراضية تلقائيًا.\n\t--xmlout, -x\t\tتبديل إلى مخرجات بتنسيق XML.\n\t--reposd-dir, -D <dir>\tاستخدام الدليل البديل لملفات\n\t\t\t\tتعريف المخزن.\n\t--cache-dir, -C <dir>\t استخدام الدليل البديل للذاكرة المؤقتة لبيانات التعريف.\n\t--raw-cache-dir <dir>\tاستخدام الدليل البديل للذاكرة المؤقتة لبيانات التعريف الأولية.\n"

#: ../src/Zypper.cc:171
msgid "     Repository Options:\n\t--no-gpg-checks\t\tIgnore GPG check failures and continue.\n\t--plus-repo, -p <URI>\tUse an additional repository.\n\t--disable-repositories\tDo not read meta-data from repositories.\n\t--no-refresh\t\tDo not refresh the repositories.\n"
msgstr "     خيارات المخزن:\n\t--no-gpg-checks\t\tتجاهل حالات فشل فحص GPG والمتابعة.\n\t--plus-repo, -p <URI>\tاستخدام مخزن إضافي.\n\t--disable-repositories\tعدم قراءة بيانات التعريف من المخازن.\n\t--no-refresh\t\tعدم تجديد المخازن.\n"

#. translators: these belong to Repository Options main help text
#: ../src/Zypper.cc:178
msgid "\t--no-cd\t\t\tIgnore CD/DVD repositories.\n\t--no-remote\t\tIgnore remote repositories.\n"
msgstr "\t--no-cd\t\t\tتجاهل مخازن الأقراص المضغوطة/أقراص الفيديو الرقمية.\n\t--no-remote\t\tتجاهل المخازن البعيدة.\n"

#: ../src/Zypper.cc:182
msgid "     Target Options:\n\t--root, -R <dir>\tOperate on a different root directory.\n\t--disable-system-resolvables\n\t\t\t\tDo not read installed packages.\n"
msgstr "     خيارات الهدف:\n\t--root, -R <dir>\tتشغيل على دليل جذر مختلف.\n\t--disable-system-resolvables\n\t\t\t\tعدم قراءة الحزم المثبَّتة.\n"

