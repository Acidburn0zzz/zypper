#. translators: -m, --metadata
#: src/Zypper.cc:2628
msgid "Clean metadata cache."
msgstr "Bersihkan cache metadata."

#. translators: -M, --raw-metadata
#: src/Zypper.cc:2630
msgid "Clean raw metadata cache."
msgstr "Bersihkan cache metadata raw."

#. translators: -a, --all
#: src/Zypper.cc:2632
